{
  "name": "fastify-nodejs-application",
  "version": "0.1.0",
  "type": "module",
  "description": "fastify-nodejs-application",
  "engines": {
    "node": ">=16.x"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "test": "jest --verbose --runInBand",
    "start": "fastify start src/app.js -l debug -P -o --watch --verbose-watch --ignore-watch='node_modules .git .sqlite'"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/__tests__/helpers/"
    ],
    "setupFiles": [
      "dotenv/config"
    ]
  },
  "author": "Dmitriy Miroshnichenko",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hexlet-boilerplates/fastify-nodejs-application.git"
  },
  "bugs": {
    "url": "https://github.com/hexlet-boilerplates/fastify-nodejs-application/issues"
  },
  "homepage": "https://github.com/hexlet-boilerplates/fastify-nodejs-application.git#readme",
  "dependencies": {
    "@fastify/cors": "^8.2.0",
    "@fastify/sensible": "^5.1.0",
    "fastify": "^4.10.2",
    "fastify-cli": "^5.1.0",
    "fastify-objectionjs": "^2.0.0",
    "knex": "^2.4.0",
    "lodash": "^4.17.21",
    "objection": "^3.0.1",
    "objection-unique": "^1.2.2",
    "pg": "^8.9.0",
    "qs": "^6.11.0",
    "sqlite3": "^5.0.11"
  },
  "devDependencies": {
    "@jest/globals": "^28.1.3",
    "@types/jest": "^28.1.7",
    "@types/node": "^18.7.11",
    "dotenv": "^16.0.1",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.7",
    "jest": "^28.1.3",
    "jest-cli": "^28.1.3",
    "pino-pretty": "^9.0.1"
  }
}
